<div class="container-fluid">
  <div class="list-group">
    <div class="list-group-item active">
      <h4 class="list-group-item-heading text-primary">
        Upcoming meetings!
      </h4>
    </div>
    <ul class="list-group">
      <% @meetup_events.each do |event| %>
        <li class="list-group-item">
          <span class="badge"> <%= event.yes_rsvp_count %> </span>
          <p> <%= event.name %> </p>
          <a href=<%= event.event_url %>><%= event.event_url %></a>
          <p> <%= event.starts_at.localtime.to_s(:long) %> - <%= event.ends_at.localtime.to_s(:long) %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
